<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment />
	<entry key="modifyCropPrice">
	    UPDATE
	           CROP C
	       SET C.CROP_PRICE = ?
	     WHERE C.CROP_ID = ?
	</entry>
	
	<entry key="modifyHousePrice">
	    UPDATE
	           USER_INFO U
	       SET U.HOUSE_PRICE = ?
	</entry>
	<entry key="selectCropPrice">
	    SELECT
	           CROP_NAME
	         , CROP_PRICE
	      FROM CROP
	</entry>
</properties>
